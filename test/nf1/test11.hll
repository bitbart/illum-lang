contract Test11 {
    int n; 
    int m;
  
    function f() { 
        if (n==0) {
            m=0;
        }
        else { 
            if (n==1) {
                m=1; 
            }
            else {
                if (n==2) {
                    m=2;
                }
            }
        } 
    }
}
